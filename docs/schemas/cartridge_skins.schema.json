{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "title": "Cartridge Skins Manifest",
  "type": "object",
  "additionalProperties": false,
  "required": [
    "version",
    "category",
    "images"
  ],
  "properties": {
    "version": {
      "type": "integer",
      "minimum": 1
    },
    "category": {
      "type": "string",
      "const": "cartridges"
    },
    "images": {
      "type": "array",
      "items": {
        "type": "object"
      }
    }
  }
}